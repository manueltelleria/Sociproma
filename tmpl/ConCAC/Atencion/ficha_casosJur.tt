    </tr>

        <tr>
        <td align="left" class="fondoetiqueta" nowrap>Unidad Responsable</td><td>[% id_responsables %]</td>
    </tr>

    <form id="forma" method="post" name="forma">
        <tr>
            <td align center  colspan=2  align=center>
                <input type=button  onclick="javascript: upd(0,'[% retorno %]')" value='<< Regresar'/>&nbsp; 
            </td>
        </tr>
        <input type=hidden name=id_ciudadanos value=[% id_ciudadanos %]>
    </form>
</table>
[% formbuilder.field.id_ciudadanos.field %]
[% formbuilder.field.id_casos.field %]
[% formbuilder.end %]
</fieldset>
</body>
</html>