<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>[% titulo %]</title>
<!-- hojas de estilo generales -->
[% INCLUDE estilosgenerales.tt %]

<!-- archivos de javascript para Prototype -->
<script type="text/javascript" src="[%ruta_estatica%]/js/prototype/prototype.js"></script>

<!-- archivos de javascript para Seleccionar Item en las Listas -->
<script type="text/javascript" src="[%ruta_estatica%]/js/seleccionarItemList.js"></script>

<!-- archivos de javascript generales (cargar en todas las páginas) -->
[% INCLUDE jsgenerales.tt %]

[% formbuilder.jshead %]



<script type="text/javascript">

    function upd(valor, ruta){
//         document.forma.id_ciudadanos.value = valor;
        document.forma.action = ruta;
        document.forma.submit();
    }

    function upd2(valor, ruta){
        document.forma.id_casos.value = valor;
        document.forma.action = ruta;
        document.forma.submit();
    }

</script>
</head>
<body onload="cargarMenu()">

[% INCLUDE encabezado.tt %]
<fieldset>
<legend>Casos de Atenci&oacute;n al Ciudadano</legend>

[% formbuilder.start %]
[% formbuilder.statetags %]
[% formbuilder.keepextras %]
[% INCLUDE jscargarmenu.tt %]
<table align="center" width="65%">
    <tr>
        <td align="center" colspan="2" class="titulodonforojo">Datos Caso</td>
    </tr>
    <tr>
        <td align="left" class="" nowrap colspan=2  align=center><b>Datos Ciudadano</b></td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>RIF</td><td>[% id_tprif %]-[% scedularif %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Nombre de la empresa</td><td>[% snombre %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Razón Social</td><td>[% sapellido %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Direcci&oacute;n</td><td>[% sdireccion %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Estado</td><td>[% id_tpentfederal %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Tel&eacute;fono</td><td>[% stelefono %] / [% stelefono2 %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Observaci&oacute;n</td><td>[% sbservacion_cd %]</td>
    </tr>
    <tr>
        <td align="left" class="" nowrap colspan=2  align=center><b>Datos del Caso</b></td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Tipo</td><td>[% id_tpcaso %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Fecha</td><td>[% dcaso %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Lugar</td><td>[% slugar %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Asunto</td><td>[% sasunto %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Descripci&oacute;n</td><td>[% sdescripcion %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Observaci&oacute;n</td><td>[% sobservacion %]</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap>Remitido por</td><td>[% id_remitidopor %]</td>
    </tr>
        <tr>
        <td align="left" class="fondoetiqueta" nowrap>Remitido para</td><td>[% id_remitidopara %]</td>
    </tr>
    </tr>
        <tr>
