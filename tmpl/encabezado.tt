<!-- INICIO MENU -->
<table width="100%" border="0" cellspacing="0" cellpadding="0">
  [% IF nombre_usuario %]
  <tr>
    <td align="right" width="100%" class="titulofondogris" nowrap>
      <div id="menu_zone" style="width:100%;border :1px solid Silver;"></div>
    </td>
  </tr>
  [% END %]
</table>
<!-- FIN MENU -->
<br/>
[% IF subtitulo %]
<div class="titulonegro" align="center">[% subtitulo %]</div>
[% END %]
<div align="center">
<span class="titulodonfomarillo" id="error_msg">[% error_msg %]</span>
</div>
<script type="text/javascript" language="Javascript">
//ocultar mensajes de error luego de 10 segundos
if (document.getElementById) {
  if (document.getElementById('error_msg')) {
    window.setTimeout("document.getElementById('error_msg').style.display = 'none'",6000);
  }
}
</script>
<script type="text/javascript" src="[% ruta_estatica %]/js/tooltip.js"></script>
