<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>[% titulo %]</title>
<!-- hojas de estilo generales -->
[% INCLUDE estilosgenerales.tt %]

<!-- archivos de javascript generales (cargar en todas las pÃ¡ginas) -->
[% INCLUDE jsgenerales.tt %]

[% formbuilder.jshead %]
<script language="javascript">

function upd(valor, ruta){
  document.forma.id_proyectos.value = valor;
  document.forma.action = ruta;
  document.forma.submit();
}

</script>
</head>
<body  onload="cargarMenu()">
[% INCLUDE encabezado.tt %]
<fieldset>
<legend>Administrar Proyectos</legend>
<!-- Login form -->
[% formbuilder.start %]
[% formbuilder.statetags %]
[% formbuilder.keepextras %]
[% INCLUDE jscargarmenu.tt %]
     <table align="center" border="0" cellspacing="1" cellpadding="2">
        <tr>

            <td colspan="11">
        <table align="left" width="100%" border="0">
        <tr>
            <td width="20%" class="fondoetiqueta" align="left" colspan="2">
            <b>[% formbuilder.field.criterio.label %]</b></td>

            <td align="left" valign="middle" colspan=9>[% formbuilder.field.criterio.field %]&nbsp;<img src="[% ruta_estatica %]/imagenes/menu/help.gif" onmouseover="Tip('<b>Criterio de Busqueda</b><br>Debe especificar un parámetro que concuerden con los campos:<br>Código del Proyecto, Solicitante y Nombre del Proyecto')" onmouseout="UnTip()"    />&nbsp;&nbsp;[% formbuilder.submit %]</td>

        </tr>
        </table>
    </td>
        </tr>
        <tr class=titulodonforojo>
            <th width=45 colspan=12>Proyectos</th>
        </tr>
        <tr class=fondoetiqueta5>
            <th width=40>N°</th>
            <th width=200>Solicitante</th>
            <th width=200>Nombre del Proyecto</th>
            <th width=70>Monto Proyecto ($)</th>
            <th width=70>Monto Financiar ($)</th>
            <th width=70>Pais</th>
            <th width=100>Ente Ejecutor</th>
            <th width=70>Sector</th>
            <th width=100>Status <img src="[% ruta_estatica %]/imagenes/menu/help.gif" onmouseover="Tip('<b>Status</b><br>Para cambiar el estatus de proyecto, haga click sobre el status')" onmouseout="UnTip()"/></th>

            <th width=80>Licitaciones</th>
            <th width=80>Desembolsos</th>
            <th width=80>Eliminar</th>

        </tr>
        [% FOREACH field IN tabla %]
        <tr class="[% field.clase %]">
            <td align="center"><a style="text-decoration:none" href="javascript:upd([% field.id %],'/coproci/proyecto/proyecto/fichaProyecto')">[% field.id %]</a></td>
            <td>[% field.ssolicitante %]</td>
            <td>[% field.snombproy %]</td>

            <td  align="right" nowrap>[% field.nmtoproyecto %]</td>
            <td  align="right" nowrap>[% field.nmtofinanc %]</td>

            <td>[% field.pais %]</td>
            <td>[% field.senteejecutor %]</td>
            <td>[% field.id_tpsector %]</td>
            <td><a style="text-decoration:none" href="javascript:document.forma.flag.value=1; upd([% field.id %],'/coproci/proyecto/estatus/entradaEstatus')">[% field.stpedoproy %]</a></td>
            <td align=center><a style="text-decoration:none" href="javascript:document.forma.flag.value=1; upd([% field.id %],'/coproci/licitacion/licitacion/indexLicitacion')">LIC</a></td>
            <td align=center><a style="text-decoration:none" href="javascript:document.forma.flag.value=1; upd([% field.id %],'/coproci/desembolso/desembolso/indexDesembolso')">DES</a></td>
            <td align=center width=30>
            <a style="text-decoration:none" href="javascript:if (confirm('Desea eliminar el registro'))upd([% field.id %],'/coproci/proyecto/proyecto/eliminarProyecto')"><IMG src="/static/imagenes/menu/delete.gif" border="0"></a></td>

        </tr>
        [% END %]

      </table>
[% formbuilder.field.id_proyectos.field %]
[% formbuilder.field.flag.field %]
[% formbuilder.end %]
</fieldset>
</body>
</html>