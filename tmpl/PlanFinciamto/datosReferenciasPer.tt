[%formbuilder.jshead%]

<script type="text/javascript">

function modificarReferenciasPer(i){
  document.datosreferenciasper.action = "[% url_modificarReferenciasPer %]";
  document.datosreferenciasper.submit();
}

function guardarReferenciasPer(){
  document.datosreferenciasper.action = "[% url_guardarReferenciasPer %]";
  document.datosreferenciasper.submit();
}
function mostrarReferenciasPer(id){
  document.datosreferenciasper.action = "[% url_editarCargasFam %]";
  document.datosreferenciasper.id_referencia.value = id;
  document.datosreferenciasper.submit();
}

function editarReferenciasPer(){
  document.datosreferenciasper.action = "[% url_editarReferenciasPer %]";
  document.datosreferenciasper.submit();
}

function conMayusculas(nombreTextoEscrito) {
  var forma = document.datosreferenciasper;
  var palabra = eval('forma.'+nombreTextoEscrito);
  palabra.value = palabra.value.toUpperCase()
}


function validaCedulaRepresentante() {
  var form = document.datosreferenciasper;

  if (form.elements['challenge1CedulaRef_0'].value != ''){
    if  (form.elements['challenge1CedulaRef_0'].value ==  form.elements['ncedula'].value){
        alert('No puede ser igual el valor de las Cedulas de Identidad del Representante Legal y el Solicitante');
        return false;
    }
  }
}
</script>
[%INCLUDE jscargarmenu.tt%]

</head>
<body onload="cargarMenu();">

[%INCLUDE encabezado.tt %]
[%formbuilder.start%]
[%formbuilder.statetags%]
[%formbuilder.keepextras%]
[% IF rrhh %]
<fieldset>
