<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>[% titulo %]</title>
<!-- hojas de estilo generales -->
<style type="text/css">
<!--
.prueba {
	background-color: #000000;
}
-->
</style>
[% INCLUDE estilosgenerales.tt %]

<!-- archivos de javascript para Prototype -->
<script type="text/javascript" src="[%ruta_estatica%]/js/prototype/prototype.js"></script>

<!-- archivos de javascript para Seleccionar Item en las Listas -->
<script type="text/javascript" src="[%ruta_estatica%]/js/seleccionarItemList.js"></script>

<!-- archivos de javascript generales (cargar en todas las pÃ¡ginas) -->
[% INCLUDE jsgenerales.tt %]

<!-- hoja de estilo del calendario -->
<link rel="stylesheet" type="text/css" media="all" href="[% ruta_estatica %]/js/jscalendar/calendar-win2k-cold-2.css" title="Calendario" />
<script type="text/javascript" src="[% ruta_estatica %]/js/jscalendar/calendar.js"></script>
<script type="text/javascript" src="[% ruta_estatica %]/js/jscalendar/lang/calendar-es.js"></script>
<script type="text/javascript" src="[% ruta_estatica %]/js/jscalendar/calendar-setup.js"></script>

[% formbuilder.jshead %]


<script language ="JavaScript">

function imprime(){
//desaparece el boton
document.getElementById("noprint").style.visibility='hidden';
//se imprime la pagina
print();
setTimeout("showlayer()",2000);
}
function showlayer(){
     document.getElementById("noprint").style.visibility='visible';
}

</script>


</head>
<body>

[% formbuilder.start %]
[% formbuilder.statetags %]
[% formbuilder.keepextras %]
[% formbuilder.sticky %]

[% INCLUDE jscargarmenu.tt %]
<table border="1" cellpadding="0" cellspacing="0" bordercolor="#000000" width="50%">
<tr bgcolor="">
   <td bordercolor="#000000" align="center" colspan=3 ><b>Unidad Administrativa</b></td>
   <td bordercolor="#000000" align="center" colspan="4">[% unidadadm %]</td>
</tr>
<tr bgcolor="">
   <td bordercolor="#000000" align="center" colspan=3 ><b>Proyecto</b></td>
   <td bordercolor="#000000" align="center" colspan="4">[% proyecto %]</td>
</tr>
<tr bgcolor="">
   <td bordercolor="#000000" align="center" colspan=3 ><b>Acci&oacute;n Espec&iacute;fica</b></td>
   <td bordercolor="#000000" align="center" colspan="4">[% accionespecifica %]</td>
</tr>
</table>
<p>
<table border="1" cellpadding="0" cellspacing="0" bordercolor="#000000">

<!--<tr bgcolor="#FFFF66">-->
<tr class="titulodonforojo">
   <td bordercolor="#000000" align="center"><strong>PA</strong></td>
   <td bordercolor="#000000" align="center"><strong>GE   </strong></td>
   <td bordercolor="#000000" align="center"><strong>ES   </strong></td>
   <td bordercolor="#000000" align="center"><strong>SE   </strong></td>
   <td bordercolor="#000000" align="center"><strong>DENOMINACI&Oacute;N   </strong></td>
   [% FOREACH field IN periodo %]
   <td class="negrita" align="center">[% field %]</td>
   [% END %]

</tr>
[% FOREACH partidas %]
[% IF tipoformato == 1  %]
<tr class="fondoetiqueta5">
   <td class="[% clase %]" align="right">[% partida %]
   </td>
   <td class="[% clase %]" align="right">[% generica %]
   </td>
   <td class="[% clase %]" align="right">[% especifica %]
   </td>
   <td class="[% clase %]" align="right">[% subespecifica %]
   </td>
   <td class="[% clase %]" align="center">[% denominacion%]
   </td>
   [% FOREACH field IN periodo %]
   <td class="[% clase %]" align="right">[% field %]
   </td>
    [% END %]
   <td class="[% clase %]" align="right">[% total%]</td>
</tr>
[% END%]
[% FOREACH lista_generica %]
[% IF tipoformato == 1  %]
    <tr>
   <td class="[% clase %]" align="right">[% partida %]
   </td >
   <td class="[% clase %]" align="right">[% generica %]
   </td>
   <td class="[% clase %]" align="right">[% especifica %]
   </td>
   <td class="[% clase %]" align="right">[% subespecifica %]
   </td>
      <td class="[% clase %]" align="center">[% denominacion %]
      </td>
      [% FOREACH field IN periodo %]
      <td class="[% clase %]" align="right">[% field %]
      </td>
      [% END %]
   <td class="[% clase %]" align="right">[% total%] </td>
   </tr>
[% END %]
   [% FOREACH lista_especifica %]
   [% IF (tipoformato == 1) || (registros == 0 and  tipoformato == 2 ) %]
   <tr>
   <td class="[% clase %]" align="right">[% partida %]
   </td>
   <td class="[% clase %]" align="right">[% generica %]
   </td>
   <td class="[% clase %]" align="right">[% especifica %]
   </td>
   <td class="[% clase %]" align="right">[% subespecifica %]
   </td>
      <td class="[% clase %]" align="center">[% denominacion %]
      </td>
      [% FOREACH field IN periodo %]
      <td class="[% clase %]" align="right">[% field %]
      </td>
      [% END %]
      <td class="[% clase %]" align="right">[% total%]</td>
   </tr>
   [% ELSE %]<!--
<TR><TD>[% tipoformato %]#[% registros %]#</TD></TR>-->
   [% END %]
   [% FOREACH lista_subespecifica %]
      <tr>
      <td class="[% clase %]" align="right">[% partida %]
      </td >
      <td class="[% clase %]" align="right">[% generica %]
      </td>
      <td class="[% clase %]" align="right">[% especifica %]
      </td>
      <td class="[% clase %]" align="right">[% subespecifica %]
      </td>
         <td class="[% clase %]" align="center">[% denominacion %]
         </td>
         [% FOREACH field IN periodo %]
         <td class="[% clase %]" align="right">[% field %]
         </td>
         [% END %]
      <td class="[% clase %]" align="right">[% total%] </td>
      </tr>
   [% END %]
   [% END %]
[% END %]
[% END %]
[% FOREACH total %]
<tr class="fondoetiqueta5">
   <td class="fondoetiqueta5" colspan="5" align="center">TOTAL
   </td>
    [% FOREACH field IN periodo %]
    <td class="[% clase %]" align="right">[% field %]
    </td>
      [% END %]
      <td class="[% clase %]" align="right">[% total%]</td>
   </tr>
[% END %]
</table>
<div id=noprint>
<TABLE width="93%" cellpadding="2">
<tr align="center">
  <td colspan="6">[% formbuilder.field.cerrar.field %]&nbsp;[% formbuilder.field.imprimir.field %]</td>
</tr>
</TABLE>
</div>
[% formbuilder.end %]

</body>
</html>