<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>[% titulo %]</title>
<!-- hojas de estilo generales -->
[% INCLUDE estilosgenerales.tt %]

<!-- archivos de javascript generales (cargar en todas las pÃ¡ginas) -->
[% INCLUDE jsgenerales.tt %]

[% formbuilder.jshead %]
<script language="javascript">

function upd(valor, ruta){
  document.forma.id_proyectos.value = valor;
  document.forma.action = ruta;
  document.forma.submit();
}

</script>
</head>
<body  onload="cargarMenu()">
[% INCLUDE encabezado.tt %]
<fieldset>
<legend>Administrar Estructura Presupuestaria</legend>
<!-- Login form -->
[% formbuilder.start %]
[% formbuilder.statetags %]
[% formbuilder.keepextras %]
[% INCLUDE jscargarmenu.tt %]
     <table align="center" border="0" cellspacing="1" cellpadding="2">
        <tr>

            <td colspan="11">
        <table align="left" width="100%" border="0">
        <tr>
            <td width="20%" class="fondoetiqueta" align="left" colspan="2">
            <b>[% formbuilder.field.criterio.label %]</b></td>

            <td align="left" valign="middle" colspan=9>[% formbuilder.field.criterio.field %]&nbsp;<img src="[% ruta_estatica %]/imagenes/menu/help.gif" onmouseover="Tip('<b>Criterio de Busqueda</b><br>Debe especificar un parámetro que concuerden con los campos:<br>Código del Proyecto y Nombre del Proyecto')" onmouseout="UnTip()"    />&nbsp;&nbsp;[% formbuilder.submit %]  <input type=button  onclick="javascript: document.forma.flag.value=0; upd(0,'/presupuesto/estructura/estructura/entradaProyecto')" value='Crear Proyecto / Acci&oacute;n Centralizada'/></td>

        </tr>
        </table>
    </td>
        </tr>
        <tr class=titulodonforojo>
            <th width=45 colspan=12>Proyectos / Acciones Centralizadas</th>
        </tr>
        <tr class=fondoetiqueta5>
            <th width=40>Cod</th>
            <th width=200>Unidad Administrativa</th>
            <th width=200>Proyectos / Acciones Centralizadas</th>
            <th width=70>Tipo</th>
            <th width=40>Acciones Especificas</th>
            <th width=80>Eliminar</th>

        </tr>
        [% FOREACH field IN tabla %]
        <tr class="[% field.clase %]">
            <td align="center"><a style="text-decoration:none" href="javascript:upd([% field.id %],'/presupuesto/estructura/estructura/entradaProyecto')">[% field.scodproy %]</a></td>
            <td>[% field.id_unidadesadmin %]</td>
            <td>[% field.snombproy%]</td>
            <td  align=center>[% field.id_tpproyecto %]</td>

            <td align=center><a style="text-decoration:none" href="javascript:document.forma.flag.value=0; upd([% field.id %],'/presupuesto/estructura/estructura/entradaAccion')">Agregar</a></td>

            <td align=center><a style="text-decoration:none" href="javascript:if (confirm('Desea eliminar el registro'))upd([% field.id %],'/presupuesto/estructura/estructura/eliminarProyecto')"><IMG src="/static/imagenes/menu/delete.gif" border="0"></a></td>
            [% END %]
        </tr>


      </table>
[% formbuilder.field.id_proyectos.field %]
[% formbuilder.field.flag.field %]
[% formbuilder.end %]
</fieldset>
</body>
</html>
