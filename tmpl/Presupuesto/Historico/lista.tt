<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>[% titulo %]</title>
<!-- hojas de estilo generales -->
[% INCLUDE estilosgenerales.tt %]

<!-- archivos de javascript generales (cargar en todas las pÃ¡ginas) -->
[% INCLUDE jsgenerales.tt %]

[% formbuilder.jshead %]
<script language="javascript">

function upd(valor, ruta){
  document.forma.id_presupuestos.value = valor;
  document.forma.action = ruta;
  document.forma.submit();
}

</script>
</head>
<body  onload="cargarMenu()">
[% INCLUDE encabezado.tt %]
<fieldset>
<legend>Saldos Hist&oacute;ricos</legend>
<!-- Login form -->
[% formbuilder.start %]
[% formbuilder.statetags %]
[% formbuilder.keepextras %]
[% INCLUDE jscargarmenu.tt %]
     <table align="center" border="0" cellspacing="1" cellpadding="2" width=600>
        <tr>

            <td colspan="11">
        <table align="left" width="600" border="0">
        <tr>
            <td width="20%" class="fondoetiqueta" align="left" colspan="2">
            <b>[% formbuilder.field.criterio.label %]</b></td>

            <td align="left" valign="middle" colspan=9>[% formbuilder.field.criterio.field %]&nbsp;<img src="[% ruta_estatica %]/imagenes/menu/help.gif" onmouseover="Tip('<b>Criterio de Busqueda</b><br>Debe especificar un parámetro que concuerden con los campos:<br>Código del Presupuesto y Año del Presupuesto')" onmouseout="UnTip()"    />&nbsp;&nbsp;[% formbuilder.submit %]  <!--<input type=button  onclick="javascript: document.forma.flag.value=0; upd(0,'/presupuesto/administracion/presupuesto/entradaPresupuesto')" value='Crear Hist&oacute;rico'/>--></td>

        </tr>
        </table>
    </td>
        </tr>
        <tr class=titulodonforojo>
            <th colspan=5>Presupuestos</th>
        </tr>
        <tr class=fondoetiqueta5>
            <th width=40>N°</th>
            <th width=40>Año</th>
<!--             <th width=120>Tipo</th> -->
<!--              <th width=100>Estatus <br>SOLO CERRADOS</th>  -->
            <th width=100>Desde</th>
            <th width=100>Hasta</th>
            <th width=150>Parametros Financieros</th>
<!--             <th width=80>Eliminar</th> -->

        </tr>
        [% FOREACH field IN tabla %]
        <tr class="[% field.clase %]">
            <td align="center"><a style="text-decoration:none" href="javascript:upd([% field.id %],'/presupuesto/historico/registrarsaldos/detallarSaldos')">[% field.id %]</a></td>
            <td align="center">[% field.ianio %]</td>
<!--             <td align="center">[% field.id_tppresupuesto %]</td> -->
<!--              <td align="center">[% field.id_tpedopresupuesto %]</td>  -->
            <td align="center">[% field.ddesde %]</td>
            <td align="center">[% field.dhasta %]</td>
            <td align=center>
                I.P.C.:<b>[% field.nipc %] %</b><br>
                I.V.A.:<b>[% field.niva %] %</b><br>
                U.T.:<b>[% field.nut %] BsF</b>
            </td>
            [% END %]
        </tr>
      </table>
[% formbuilder.field.id_presupuestos.field %]
[% formbuilder.field.flag.field %]
[% formbuilder.end %]
</fieldset>
</body>
</html>