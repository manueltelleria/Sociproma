[% formbuilder.jshead %]
<script type="text/javascript">
    function upd(ruta){                
//                 document.forma.id_tpregionpais.value = region; 
                document.forma.action = ruta;
                document.forma.submit();
    }
    function resumen(){                
//                 document.forma.id_tpregionpais.value = region; 
                if (document.forma.id_tpregionpais1.value){
                    document.forma.id_tpregionpais.value=document.forma.id_tpregionpais1.value;
                    document.forma.action = '/coprofi/reporte/guacho/resumenAsignacionPais';
                }else{
                    document.forma.action = '/coprofi/reporte/guacho/resumenAsignacion';
                    
                }
                
                document.forma.submit();
    }
    function situacion(){                
 
                if (document.forma.id_tpregionpais2.value){
                    document.forma.id_tpregionpais.value=document.forma.id_tpregionpais2.value;                 
                }
                document.forma.action = '/coprofi/reporte/guacho/situacionFaci';
                document.forma.submit();
    }

    function distribucion(){                

                if (document.forma.id_tpregionpais3.value){
                    document.forma.id_tpregionpais.value=document.forma.id_tpregionpais3.value;                 
                }
                document.forma.action = '/coprofi/reporte/guacho/distribucionCartera';
                document.forma.submit();
    }

</script>
</head>
<body  onload="cargarMenu()">
[% INCLUDE encabezado.tt %]
<fieldset>
<legend>Guachos</legend>
<!-- Login form -->
[% formbuilder.start %]
[% formbuilder.statetags %]
[% formbuilder.keepextras %]
[% INCLUDE jscargarmenu.tt %]
     <table align="center" border=0 cellspacing="1" cellpadding="2" width=600 >
         <tr class=titulodonforojo>
            <td align=center colspan=3> 
                Reportes especiales
            </td>             
        </tr>        
        <tr class=fondoetiqueta5>
            <td align=center > 
                Tipo de Reporte
            </td> 
            <td align=center width=200> 
                Parametro de Consulta: (Regi&oacute;n)
            </td>             
            <td align=center width=200> 
                Ejecutar
            </td> 
            
        </tr>
        <tr>
            <td align=center width=200> 
                Resumen de la Asignación de recursos
            </td> 
            <td align=center > 
                [% formbuilder.field.id_tpregionpais1.field %] 
            </td> 
            <td align=center> 
                <input type=button  onclick="javascript:resumen()" value='Resumen'/>                 
            </td>
             
        </tr>
        <tr class=fondoetiqueta>
            <td align=center width=200> 
                Situación de los Proyectos del Financiamiento
            </td> 
            <td align=center > 
                [% formbuilder.field.id_tpregionpais2.field %] 
            </td> 
            <td align=center> 
                <input type=button  onclick="javascript:situacion()" value='Situación'/> 
            </td> 
        </tr>
        <tr>
            <td align=center width=200> 
                Distribución de la Cartera de Proyectos por Tipo de Operación 
            </td> 
            <td align=center > 
                 [% formbuilder.field.id_tpregionpais3.field %] 
            </td> 
            <td align=center> 
                <input type=button  onclick="javascript:distribucion()" value='Distribución'/>
            </td> 
        </tr>
        <tr class=fondoetiqueta2>
            <td align=center colspan=3> 
                <b>* Si no se selecciona una regi&oacute;n se ejecutara el reporte general.
            </td>             
        </tr>   
    </table>
     [% formbuilder.field.id_tpregionpais.field %]
    [% formbuilder.end %]
