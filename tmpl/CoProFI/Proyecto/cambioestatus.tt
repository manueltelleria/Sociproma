<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>[% titulo %]</title>
<!-- hojas de estilo generales -->
[% INCLUDE estilosgenerales.tt %]

<!-- archivos de javascript generales (cargar en todas las páginas) -->
[% INCLUDE jsgenerales.tt %]

[% formbuilder.jshead %]
<script type="text/javascript">
    function upd(valor, ruta){
                document.forma.id_proyectos.value = valor;
                document.forma.action = ruta;
                document.forma.submit();
    }
</script>
</head>
<body onload="cargarMenu()">

[% INCLUDE encabezado.tt %]
<fieldset>
<legend>Cambio estatus</legend>
[% formbuilder.start %]
[% formbuilder.statetags %]
[% formbuilder.keepextras %]
[% INCLUDE jscargarmenu.tt %]
<table align="center" width="65%">
    <tr>
        <td align="center" colspan="2" class="titulodonforojo">Cambio de estatus del Proyecto</td>
    </tr>
    <tr>
        <td align="left" class="fondoetiqueta" nowrap><b>[% formbuilder.field.id_proyectos.label %]</b></td>
        <td align="left"><b>[% formbuilder.field.id_proyectos.value %]</b>[% formbuilder.field.id_proyectos.field %]</td>
    </tr>

    <tr>
        <td align="left" class="fondoetiqueta" nowrap><b>[% formbuilder.field.ssolicitante.label %]</b></td>
        <td align="left"><b>[% formbuilder.field.ssolicitante.value %]</b></td>
    </tr>


    <tr>
        <td align="left" class="fondoetiqueta" nowrap><b>[% formbuilder.field.snombproy.label %]<b></td>
        <td align="left"><b>[% formbuilder.field.snombproy.value %]</b></td>
    </tr>

    <tr>
        <td align="left" class="fondoetiqueta" nowrap><b>[% formbuilder.field.id_tpedoproy.label %]<b></td>
        <td align="left">[% formbuilder.field.id_tpedoproy.field %]&nbsp;(*)
    </tr>

    <tr>
        <td align="left" class="fondoetiqueta" nowrap><b>[% formbuilder.field.sobservaciones.label %]<b></td>
        <td align="left">[% formbuilder.field.sobservaciones.field %]</td>
    </tr>


    <tr class=''>
      <td colspan=2 >(*) Estos campos son obligatorios</td>
    </tr>
  <tr>
    <td colspan="2" align="center">
        <input type=submit  onclick="javascript:document.forma.flag2.value = '';" value='Guardar'/> &nbsp;
        <input type=button  onclick="javascript:upd(0,'/coprofi/proyecto/proyecto/listar')" value='<< Regresar'/>
    </td>
  </tr>
</table>
<br><br><br>
 <table align="center" border=0 cellspacing="1" cellpadding="2" width="65%">
        <tr class=titulodonforojo>
            <th colspan=5> Historia de cambios y transacciones en el Proyecto</th>
        </tr>
        <tr class=fondoetiqueta5>
            <th width=30>N°<br> Transacción</th>
            <th width=100>Estatus</th>
            <th width=70>Observaci&oacute;n</th>
            <th width=20>Fecha/Hora</th>
            <th width=40>Funcionario <br>ejecutor</th>
        </tr>
        [% FOREACH field IN tabla %]
        <tr class=[% field.clase %]>
            <td align=center>[% field.id %]</td>
            <td>[% field.id_tpedoproy %]</td>
            <td>[% field.sobservaciones %]</td>
            <td>[% field.tcreacion %]</td>
            <td>[% field.usuario %]</td>
            
        </tr>
        [% END %]
        <tr >
            <tD colspan=11><hr><input type=submit  onclick="javascript:document.forma.flag2.value = 'XLS';" value='Generar XLS'/></tD>            
        </tr>
      </table>

[% formbuilder.field.flag.field %]
[% formbuilder.field.flag2.field %]
[% formbuilder.end %]
</fieldset>
</body>
</html>