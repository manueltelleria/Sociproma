<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>

<!-- hojas de estilo generales -->
[% INCLUDE estilosgenerales.tt %]

[%formbuilder.jshead%]

<script language ="JavaScript">

function imprime(){
//desaparece el boton
document.getElementById("noprint").style.visibility='hidden';
//se imprime la pagina
print();
setTimeout("showlayer()",2000);
}
function showlayer(){
     document.getElementById("noprint").style.visibility='visible';
}
</script>
</head>
<br>
[%formbuilder.start%]
<div class="page">
<table width="100%" cellspacing="0" cellpadding="1" border="0">
  <tr>
    <td align="left">
      <center><b>FACTORES A EVALUAR PERSONAL DE OBREROS</b></center>
    </td>
  </tr>
</table>
<br/>
<table width="100%" align="center" cellspacing="0" cellpadding="2" border="1">
[% FOREACH listado %]
<tr>
  <td colspan="5" height="15px" align="center" bgcolor="#DDDDDD"><b>[% nroCateg %].-[% categoria %]</b></td>
</tr>
<tr>
[% FOREACH strango %]
  <td height="95px" align="justify" height="25px" valign="top"><small>[% descrip %]</small></td>
[% END %]
</tr>
<tr>
[% FOREACH strango %]
  [% IF selected %]
  <td align="center"><img src="[% ruta_estatica %]/imagenes/menu/cerrar.gif" width="13" height="13" align="center" /></td>
  [% ELSE %]
  <td align="center"><img src="[% ruta_estatica %]/imagenes/menu/vacio.gif" width="13" height="13" align="center" /></td>
  [% END %]
[% END %]
</tr>
[% END %]
</table>
<div id=noprint>
  <TABLE width="93%" cellpadding="2">
    <tr align="center">
      <td colspan="6">[% formbuilder.field.btnvolver.field %][% formbuilder.field.btnImprimir.field %][% formbuilder.submit %]</td>
    </tr>
  </TABLE>
</div>
</div>
[% formbuilder.field.ncedula.field %]
[% formbuilder.end %]
<br/>
</body>
</html>
